<script type="text/javascript">
	//scroll to top
		$(window).scroll(function(){
			if ($(this).scrollTop() < 100) {
				$('.scrolldown').fadeIn();
				} else {
				$('.scrolldown').fadeOut();
			}
		});
		$('.scrolldown').click(function(){
			$("html, body").animate({ scrollTop: 620 }, 1000);
				return false;
		});

</script>
